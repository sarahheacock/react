{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9",
        ]
      }
    }],
    "react"
  ],
  "plugins": [
    [
      "add-module-exports",
      "transform-runtime", {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
